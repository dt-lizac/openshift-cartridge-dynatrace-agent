#!/bin/bash -e

source $OPENSHIFT_CARTRIDGE_SDK_BASH

function status() {
  client_result "Dynatrace agent is enabled."
}

function start() {
  env_dir="${OPENSHIFT_DYNATRACE_AGENT_DIR}env"
  client_result "Dynatrace agent started."
}


case "$1" in
  status)	status ;;
  start)	start ;;
  *)		exit 0;
esac
